datasource db {
  provider = "mysql"
  url      = env("mysql://root@localhost:3306/timeboxing")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int    @id @default(autoincrement())
  name       String
  email      String @unique
  password   String
  start_time String
  end_time   String
  status     Int
}
